{
    "name": "liip/roger-q",
    "type": "project",
    "description": "Tool to work with RabbitMQ queues, including commands to dump, deduplicate and publish messages",
    "keywords": ["rabbitmq", "queue"],
    "license": "GPL-3.0",
    "require": {
        "php": "^7.1",
        "enqueue/amqp-bunny": "^0.8.40",
        "guzzlehttp/guzzle": "^6.3",
        "pnz/json-exception": "^0.1.0",
        "symfony/console": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/roger-q.php"
    ]
}
