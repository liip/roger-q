{
    "name": "liip/roger-q",
    "type": "project",
    "description": "Tool to work with RabbitMQ queues, including commands to dump, deduplicate and publish messages",
    "keywords": ["rabbitmq", "queue"],
    "license": "GPL-3.0",
    "require": {
        "php": "^7.4 || ^8.0",
        "enqueue/amqp-bunny": "^0.10.9",
        "guzzlehttp/guzzle": "^6.5.8",
        "pnz/json-exception": "^1.0.0",
        "symfony/console": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "bin": [
        "bin/roger-q.php"
    ]
}
